<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation= "http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:component-scan base-package= "Maintenence" />


    <bean id="TestProblem" class="Maintenence.Problem" scope="prototype"></bean>

    <bean id="request" class="Maintenence.Requests" scope="prototype">
        <property name="attached_problem" ref="TestProblem" />
    </bean>

    <bean id="maintenencemaster" class="Maintenence.MaintenenceMaster" scope="prototype">
        <property name="requests_for_fac">
            <list/>
            <!--
             <list>
                 <ref bean="request" />
            </list>
            -->
        </property>
        <property name="maintenences">
            <list/>
            <!--
             <list>
                 <ref bean="request" />
            </list>
            -->
        </property>
    </bean>

</beans>




